<?xml version="1.0"?>
<!DOCTYPE tpconfig SYSTEM "../tpconfig.dtd">
<tpconfig type="installed">
    <server name="tpserver-cpp">
        <ruleset name="mtsec">
            <longname>MTSec</longname>
            <version>0.0</version>
            <description>The second milestone game. It is designed to both be a fun game and to exercise the design capabilities of the framework.</description>
      <parameter name="gamedata" type="F">
        <longname>Gamedata</longname>
        <description>Gamedata file to load.</description>
        <default />
        <commandstring>--gamedata_file %s</commandstring>
      </parameter>
        </ruleset>
    </server>
</tpconfig>

